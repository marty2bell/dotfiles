# ls
alias ls='ls -Gp'
alias ll='ls -ohGF'
alias l='ls -lGa'

# Other Commands
alias untar='tar -xvf'
alias mkdir='mkdir -p'
alias vi='vim'

# Directories
alias c='cd ..'
alias dev='cd ~/dev/'

# Files
alias editbash='vim ~/.bashrc'
alias editalias='vim ~/.bash_aliases'
alias editprof='vim ~/.bash_profile'
alias hosts='sudo vim /etc/hosts'
alias vimrc='vim ~/.vimrc'
alias f5='. ~/.bash_aliases'
